<?xml version="1.0" encoding="utf-8" ?>
<configuration>
<system.webServer>
<rewrite>
<rules>
<rule name="rule 1X">
<match url="^inc/.*$"  />
<action type="Rewrite" url="/index.php"  />
</rule>
<rule name="rule 2X" stopProcessing="true">
<match url="^(.*)$"  />
<action type="Rewrite" url="/index.php"  appendQueryString="true" />
</rule>
</rules>
</rewrite>
</system.webServer>
</configuration>